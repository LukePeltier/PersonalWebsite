{
  "name": "website-app",
  "version": "0.0.1",
  "scripts": {
    "dev": "yarn run vite dev",
    "build": "yarn run vite build",
    "package": "yarn run svelte package",
    "preview": "yarn run vite preview",
    "test": "yarn run playwright test",
    "check": "yarn run svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "yarn run svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "yarn run prettier --check --plugin-search-dir=. . && eslint .",
    "format": "yarn run prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@iconify/icons-akar-icons": "^1.2.10",
    "@iconify/svelte": "^3.0.0",
    "@playwright/test": "^1.22.2",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-node": "^1.0.0-next.100",
    "@sveltejs/kit": "next",
    "@sveltejs/package": "^1.0.0-next.5",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.0"
  },
  "type": "module",
  "dependencies": {
    "daisyui": "^2.22.0",
    "gridjs": "^5.1.0",
    "gridjs-svelte": "^2.1.1",
    "pocketbase": "^0.7.4",
    "theme-change": "^2.2.0"
  },
  "packageManager": "yarn@3.2.4"
}
